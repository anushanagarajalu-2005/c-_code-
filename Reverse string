#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int start, end;
    char temp;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    // Remove the newline character added by fgets
    str[strcspn(str, "\n")] = '\0';

    start = 0;
    end = strlen(str) - 1;

    // Swap characters from both ends moving toward the center
    while (start < end) {
        temp = str[start];
        str[start] = str[end];
        str[end] = temp;

        start++;
        end--;
    }

    printf("Reversed string: %s\n", str);

    return 0;
}
o/p:
Enter a string: anusha
Reversed string: ahsuna
